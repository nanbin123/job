/* pages/personalCenter/personalCenter.wxss */
page{
  width: 100%;
}
.personalData{
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.nameData {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 4rpx solid #f2f2f2;
}

.personalcontent {
  width: 300rpx;
}

.name {
  display: block;
  font-size: 60rpx;
  font-weight: bold;
}

.year {
  color: #676767;
  font-size: 32rpx;
  font-weight: bold;

}

.userAvatar {
  width: 150rpx;
  height: 150rpx;
  background: saddlebrown;
  display: inline-block;
  border-radius: 50%;
}
.state{
  width: 100%;
  border-bottom: 4rpx solid #f2f2f2;
  padding-bottom: 50rpx;

}
.titleText{
  color: #000;
  font-size: 34rpx;
  position: relative;
  margin-bottom: 20rpx;
}
.stateTitle{
  justify-content: space-between;
  padding: 30rpx 0 0rpx; 
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  position: relative;
}
.stateTitleL{
  color: #000;
  font-size: 34rpx;
  font-weight: bold;

}
.stateTitleR{
  color: #ababab;
  font-size: 30rpx;
}
.stateYears{
  padding-bottom: 30rpx;
  padding-top: 6rpx;
}
.iconfont{
  font-size: 32rpx;
  display: inline-block;
  margin-right: 30rpx;
  color: #5c5c5c
}
.item{
  display: inline-block;
  margin-left: 10rpx;
}
.stateNav{
  color: #5c5c5c;
  font-size: 32rpx;
}
.Resume {
  width: 100%;
  border-bottom: 4rpx solid #f2f2f2;
}
.title {
  width: 100%;
  padding: 30rpx 0 6rpx; 
  display: flex;
  align-items: center;
  position: relative;
}

.jobExpectations {
  width: 100%;
  padding-bottom: 60rpx;
  border-bottom: 4rpx solid #f2f2f2;
}

.expectationsContent {
  width: 100%;
  /* display: flex;
  justify-content: space-between;
  align-items: center; */
}

.expectationsbox {
  width: 100%;
  padding-bottom: 30rpx;
}

.expectationsList {
  width: 100%;
  margin-bottom: 20rpx;

}

.expectationsItem1 {
  font-weight: bold;
  font-size: 32rpx;
}

.expectationsItem2 {
  color: #6e6e6e;
  font-size: 32rpx;
  width: 100%;
}
.titleIco1 {
  display: inline-block;
  float: right;
  color: #ababab;
  font-size: 30rpx;
}
.money {
  display: inline-block;
  float: right;
  color: #ad3434;
  font-size: 30rpx;
  font-weight: bold;
}
.workExperience{
  width: 100%;
  

  border-bottom: 4rpx solid #f2f2f2;
}
.workExperienceContent{
  padding-bottom: 30rpx;
}
.Strength {
  font-size: 32rpx;
  line-height: 50rpx;
  color: #6d6d6d;
  width: 100%;
}
.littlebtn{
  margin-top: 30rpx;
}
.btnItem{
  color: #000;
  font-size: 32rpx;
  display: inline-block;
  margin-right: 20rpx;
  padding: 6rpx 20rpx;
  border: 4rpx solid #eaeaea;
  border-radius: 10rpx;
}

.educationExperience{
  width: 100%;
  padding-bottom: 50rpx;
  border-bottom: 4rpx solid #f2f2f2;
}
.educationExperienceBox{
  margin-bottom: 20rpx;
}
.icon{
  position: absolute;
  top: 40rpx;
  left: -28rpx;
  color: #9b3737;

}
.icon1{
  position: absolute;
  top: 36rpx;
